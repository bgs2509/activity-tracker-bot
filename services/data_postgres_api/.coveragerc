[run]
source = src
branch = true
omit =
    */tests/*
    */__pycache__/*
    */.venv/*
    */venv/*

[report]
precision = 2
show_missing = true
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov

[xml]
output = coverage.xml
